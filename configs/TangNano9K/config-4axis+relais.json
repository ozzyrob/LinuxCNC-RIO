{
    "name": "TangNano9K",
    "description": "TangNano9K with 5Axis BOB over SPI (4Axis+Relais)",
    "board": "TangNano9K",
    "type": "GW1NR-LV9QN88PC6/I5",
    "family": "GW1N-9C",
    "toolchain": "icestorm",
    "package": "",
    "clock": {
        "speed": "27000000",
        "pin": "52"
    },
    "transport": "SPI",
    "axis": 4,
    "interface": [
        {
            "type": "spi",
            "pins": {
                "MOSI": "48",
                "MISO": "49",
                "SCK": "31",
                "SEL": "32"
            }
        }
    ],
    "uart": {
        "pin_rx": "6",
        "pin_tx": "9",
        "baud": "115200"
    },
    "blink": {
        "pin": "10"
    },
    "error": {
        "invert": true,
        "pin": "11"
    },
    "enable": {
        "pin": "69"
    },
    "dout": [
        {
            "name": "flood-enable",
            "net": "iocontrol.0.coolant-flood",
            "comment": "P16",
            "invert": true,
            "pin": "57"
        },
        {
            "name": "spindle-enable",
            "net": "spindle.0.on",
            "comment": "P17",
            "invert": true,
            "pin": "56"
        }
    ],
    "din": [
        {
            "name": "home-x",
            "net": "joint.0.home-sw-in",
            "comment": "BOB DIN0",
            "_invert": true,
            "pullup": true,
            "pin": "33"
        },
        {
            "name": "home-y",
            "net": "joint.1.home-sw-in",
            "comment": "BOB DIN1",
            "_invert": true,
            "pullup": true,
            "pin": "30"
        },
        {
            "name": "home-z",
            "net": "joint.2.home-sw-in",
            "comment": "BOB DIN2",
            "_invert": true,
            "pullup": true,
            "pin": "29"
        },
        {
            "name": "tool-probe",
            "net": "motion.probe-input",
            "comment": "BOB DIN3",
            "_invert": true,
            "pullup": true,
            "pin": "28"
        },
        {
            "comment": "BOB DIN4",
            "_invert": true,
            "pullup": true,
            "pin": "68"
        },
        {
            "comment": "ADPATER DIN0",
            "pullup": true,
            "pin": "79"
        },
        {
            "comment": "ADPATER DIN1",
            "pullup": true,
            "pin": "80"
        },
        {
            "comment": "ADPATER DIN2",
            "pullup": true,
            "pin": "81"
        },
        {
            "comment": "ADPATER DIN3",
            "pullup": true,
            "pin": "82"
        },
        {
            "comment": "ADPATER DIN4",
            "pullup": true,
            "pin": "83"
        },
        {
            "comment": "ADPATER DIN5",
            "pullup": true,
            "pin": "84"
        },
        {
            "comment": "ADPATER DIN6",
            "pullup": true,
            "pin": "85"
        },
        {
            "comment": "ADPATER DIN7",
            "pullup": true,
            "pin": "86"
        }
    ],
    "vout": [
        {
            "name": "spindle-speed",
            "net": "spindle.0.speed-out",
            "min": "0",
            "max": "20000",
            "_invert": true,
            "type": "pwm",
            "pin": "55"
        }
    ],
    "vin": [
        {
            "type": "counter",
            "pullup": true,
            "pin_up": "3",
            "pin_down": "4"
        }
    ],
    "joints": [
        {
            "type": "stepper",
            "cl": false,
            "scale": 800,
            "pins": {
                "step": "54",
                "dir": "53"
