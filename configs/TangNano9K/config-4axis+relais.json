    "expansion": [
        {
            "type": "shiftreg",
            "bits": 8,
            "invert": true,
            "pullup": true,
            "pins": {
                "out": "73",
                "in": "72",
                "clock": "71",
                "load": "70"
            }
        }
    ]
}
