{
    "name": "ICEBreakerV1.0e",
    "description": "Small and low cost FPGA educational and development board",
    "toolchain": "icestorm",
    "family": "ice40",
    "type": "up5k",
    "package": "sg48",
    "flashcmd": "openFPGALoader -b ice40_generic rio.bin -f",
    "clock": {
        "osc": "12000000",
        "speed": "50250000",
        "pin": "35"
    },
    "blink": {
        "pin": "37"
    },
    "error": {
        "invert": true,
        "pin": "11"
    }
}
